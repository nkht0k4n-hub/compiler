
    INITIAL_GP = 0x10008000
    INITIAL_SP = 0x7ffffffc
    stop_service = 99
    .text
init:
    la $gp, INITIAL_GP
    la $sp, INITIAL_SP
    jal main
    nop
    li $v0, stop_service
    syscall
    nop
stop:
    j stop
    nop
    .text 0x00001000
main:
    la $t0, RESULT
#assigned matrix1 
    li $v0,1
    addi $t6,$t0,0
#2ndim number
    li $s6,0
#2ndim number
    li $s7,0
#second_dim_calucurate
    li $s5,2
    mult $s6,$s5
    mflo $s5
    nop
    li $s6,4
    mult $s7,$s6
    mflo $s7
    nop
    add $s5,$s5,$s7
    add $t6,$t6,$s5
#assign
    sw $v0, 0($t6)
    nop
#assigned matrix1 
    li $v0,2
    addi $t6,$t0,0
#2ndim number
    li $s6,0
#2ndim number
    li $s7,1
#second_dim_calucurate
    li $s5,2
    mult $s6,$s5
    mflo $s5
    nop
    li $s6,4
    mult $s7,$s6
    mflo $s7
    nop
    add $s5,$s5,$s7
    add $t6,$t6,$s5
#assign
    sw $v0, 0($t6)
    nop
#assigned matrix1 
    li $v0,3
    addi $t6,$t0,0
#2ndim number
    li $s6,1
#2ndim number
    li $s7,0
#second_dim_calucurate
    li $s5,2
    mult $s6,$s5
    mflo $s5
    nop
    li $s6,4
    mult $s7,$s6
    mflo $s7
    nop
    add $s5,$s5,$s7
    add $t6,$t6,$s5
#assign
    sw $v0, 0($t6)
    nop
    jr $ra
    nop
    .data 0x10004000
RESULT:

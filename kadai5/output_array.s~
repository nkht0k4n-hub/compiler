
    INITIAL_GP = 0x10008000
    INITIAL_SP = 0x7ffffffc
    stop_service = 99
    .text
init:
    la $gp, INITIAL_GP
    la $sp, INITIAL_SP
    jal main
    nop
    li $v0, stop_service
    syscall
    nop
stop:
    j stop
    nop
    .text 0x00001000
main:
    la $t0, RESULT
id=1
id=2
offset_count=2
id=1
id=2
offset_count=4
#assigned a 
    li $v0,1
    addi $t6,$t0,4
#assign
    sw $v0, 0($t6)
    nop
#assigned b 
    li $v0,2
    addi $t6,$t0,8
#assign
    sw $v0, 0($t6)
    nop
    jr $ra
    nop
    .data 0x10004000
RESULT:
